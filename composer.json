{
  "name": "spiral/scaffolder",
  "type": "module",
  "license": "MIT",
  "description": "Spiral, Code Scaffolding module",
  "authors": [
    {
      "name": "Anton Titov / Wolfy-J",
      "email": "wolfy.jd@gmail.com"
    }
  ],
  "require": {
    "spiral/framework": "^1.0"
  },
  "require-dev": {
    "spiral/odm": "^1.0",
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\": "source/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\Tests\\": "tests/",
      "TestApplication\\": "tests/-app-/classes/"
    }
  }
}
